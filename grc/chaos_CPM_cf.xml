<block>
  <name>CPM</name>
  <key>chaos_CPM_cf</key>
  <category>[CHAOS]</category>
  <import>import chaos</import>
  <make>chaos.CPM_cf($seed, $seq_len, $constell_tab, $para_tab)</make>
  <param>
    <name>Seed</name>
    <key>seed</key>
    <type>float</type>
  </param>
  <param>
    <name>Chaotic sequence length</name>
    <key>seq_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Constellation table</name>
    <key>constell_tab</key>
    <type>complex_vector</type>
  </param>
  <param>
    <name>Parameter table</name>
    <key>para_tab</key>
    <type>real_vector</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
